{
  "rules": {
    "max-depth": ["warn", 1],
    "max-params": ["warn", 3],
    "max-lines-per-function": ["warn", { "max": 10 }]
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["prettier", "airbnb"]
}
